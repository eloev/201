        app:layout_constraintTop_toTopOf="@id/label_nutella" />

    <TextView
        android:id="@+id/label_oreo"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_nutella"
        tools:text="oreo x2" />

    <Button
        android:id="@+id/minus_oreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/minus"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@id/label_oreo"
        app:layout_constraintTop_toTopOf="@id/label_oreo" />

    <Button
        android:id="@+id/plus_oreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/plus"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@id/label_oreo"
        app:layout_constraintTop_toTopOf="@id/label_oreo" />

    <TextView
        android:id="@+id/label_picnic"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
