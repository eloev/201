<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.pass.BasketFragment">

    <TextView
        android:id="@+id/label_j7"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="j7 x2" />

    <Button
        android:id="@+id/minus_j7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/minus"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@id/label_j7"
        app:layout_constraintTop_toTopOf="@id/label_j7" />

    <Button
        android:id="@+id/plus_j7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/plus"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@id/label_j7"
        app:layout_constraintTop_toTopOf="@id/label_j7" />

    <TextView
        android:id="@+id/label_kitkat"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_j7"
        tools:text="kitkat x2" />

    <Button
        android:id="@+id/minus_kitkat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/minus"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@id/label_kitkat"
        app:layout_constraintTop_toTopOf="@id/label_kitkat" />

    <Button
        android:id="@+id/plus_kitkat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/plus"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@id/label_kitkat"
        app:layout_constraintTop_toTopOf="@id/label_kitkat" />

    <TextView
        android:id="@+id/label_lipton"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_kitkat"
        tools:text="lipton x2" />

    <Button
        android:id="@+id/minus_lipton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/minus"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@id/label_lipton"
        app:layout_constraintTop_toTopOf="@id/label_lipton" />

    <Button
        android:id="@+id/plus_lipton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/plus"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@id/label_lipton"
        app:layout_constraintTop_toTopOf="@id/label_lipton" />

    <TextView
        android:id="@+id/label_milk"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_lipton"
        tools:text="milk x2" />

    <Button
        android:id="@+id/minus_milk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
